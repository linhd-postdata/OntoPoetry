###  http://postdata.linhd.uned.es/ontology/postdata-core#audicenceEducationLevel
:audicenceEducationLevel rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://www.cidoc-crm.org/cidoc-crm/P2_has_type> ;
                         rdfs:domain :Redaction ;
                         rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                      owl:hasValue <http://postdata.linhd.uned.es/kos/education-level>
                                    ] ;
                         rdfs:comment "Level of education of the recipients of a Redaction.nnTaking as example the play \"La segunda Celestina\" (Agustín de Salazar), \"audienceEducationLevel\" would be a learned audience."@en ;
                         rdfs:label "audience education level"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#audience
:audience rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.cidoc-crm.org/cidoc-crm/P2_has_type> ;
          rdfs:domain :Redaction ;
          rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
          rdfs:comment "The meant readership, viewers or listeners of a Redaction. There is at most one possible value for audience.nnTaking as example the play \"La segunda Celestina\", \"audience\" would be the Spanish Queen and the people in the court attending the Queen's birthday celebrations."@en ;
          rdfs:label "audience"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel
:authorEducationLevel rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.cidoc-crm.org/cidoc-crm/P2_has_type> ;
                      rdfs:domain [ owl:intersectionOf ( :CreatorRole
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasAgent ;
                                                           owl:allValuesFrom :Person
                                                         ]
                                                       ) ;
