          owl:equivalentProperty <http://www.cidoc-crm.org/cidoc-crm/P141_assigned> ;
          owl:inverseOf :wasAssignedBy ;
          rdfs:domain :AttributeAssignment ;
          rdfs:range :CRM_Entity ;
          rdfs:comment "This property indicates the attribute that was assigned to in an Attribute assignment action."@en ;
          rdfs:label "assigned"@en .


