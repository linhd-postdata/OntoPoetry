###  http://postdata.linhd.uned.es/ontology/postdata-core#assessedCertainty
:assessedCertainty rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.cidoc-crm.org/cidoc-crm/P40_observed_dimension> ;
                   owl:inverseOf :isCertaintyAssessedBy ;
                   rdfs:domain :CertaintyAssessment ;
                   rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                       owl:hasValue <http://postdata.linhd.uned.es/kos/certainty>
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
                   rdfs:comment "This property is used to express the level of confidence or certainty resulted in a Certainty Assessment activty."@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                   rdfs:label "assessed certainty"@en .


