  <rdf:Description rdf:about="&raop;interpretation">
    <rdfs:label>The interpretation of this rethorical device</rdfs:label>
    <rdfs:comment></rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&raop;" />
    <rdf:type rdf:resource="&rdfs;Property" />
