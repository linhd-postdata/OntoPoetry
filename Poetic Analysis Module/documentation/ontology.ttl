###  http://postdata.linhd.uned.es/ontology/postdata-poeticAnalysis#Foot
:Foot rdf:type owl:Class ;
      rdfs:subClassOf :AnalyticalUnit ,
                      <http://purl.org/ontology/olo/core#Slot> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :clausula ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                             owl:hasValue <http://postdata.linhd.uned.es/kos/clausula>
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ]
                      ] ;
      rdfs:comment "A unit of poetic meter consisting of stressed and unstressed syllables or long and short syllables in any of various set combinations."@en ;
      rdfs:label "Foot"@en ;
      rdfs:seeAlso "https://ahdictionary.com/word/search.html?q=foot" ,
                   "https://www.poetryfoundation.org/learn/glossary-terms/foot" .


