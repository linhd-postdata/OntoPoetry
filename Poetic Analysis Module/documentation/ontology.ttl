                                                                rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#AgentRole> ;
                                                                rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Agent> ;
                                                                rdfs:label "has agent"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasAgentRole
<http://postdata.linhd.uned.es/ontology/postdata-core#hasAgentRole> rdf:type owl:ObjectProperty ;
                                                                    owl:inverseOf <http://postdata.linhd.uned.es/ontology/postdata-core#isAgentRoleOf> ;
                                                                    rdfs:domain :ScansionProcess ;
                                                                    rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#AgentRole> ;
                                                                    rdfs:comment "This property points to the agent that participated in the scansion event."@en ;
                                                                    rdfs:label "has agent role"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isAgentOf
<http://postdata.linhd.uned.es/ontology/postdata-core#isAgentOf> rdf:type owl:ObjectProperty ;
                                                                 rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Agent> ;
                                                                 rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#AgentRole> ;
                                                                 rdfs:label "is agent of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isAgentRoleOf
<http://postdata.linhd.uned.es/ontology/postdata-core#isAgentRoleOf> rdf:type owl:ObjectProperty ;
                                                                     rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#AgentRole> ;
                                                                     rdfs:range :ScansionProcess ;
                                                                     rdfs:label "is agent role of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-poeticAnalysis#affectsFirstWord
:affectsFirstWord rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isFirstWordAffectedBy ;
                  rdfs:domain :Metaplasm ;
                  rdfs:range :Word ;
                  rdfs:comment """This property points to the first of the words affected by a metaplasm.

For example, if we consider the words \"sparse il\" in the line from the sonnet I from the Canzoniere by Petrarca, namely \"Voi ch'ascoltate in rime sparse il suono\", the first of the words being affected by a metaplasm (synaloepha in this case) is \"sparse\"."""@en ;
                  rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements" ;
                  rdfs:label "affects first word"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-poeticAnalysis#affectsLine
:affectsLine rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLineAffectedBy ;
             rdfs:domain :Enjambment ;
             rdfs:range :Line ;
             rdfs:label "affects line" .


