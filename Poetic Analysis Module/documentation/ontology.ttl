:LineList rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/ontology/olo/core#OrderedList> ;
          rdfs:comment "A Line List is an ordered list of slots of type Line that is used to represent the contents of a Redaction. Any Redaction has a Line List composed by a minimum of one Line."@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-poeticAnalysis#" ;
          rdfs:label "Line List"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-poeticAnalysis#LinePattern
:LinePattern rdf:type owl:Class ;
             rdfs:subClassOf :Pattern ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :feetType ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                                    owl:hasValue <http://postdata.linhd.uned.es/kos/feet-type>
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :accentedVowels ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :altPatterningMetricalScheme ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :altSyllabicMetricalScheme ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :grammaticalStressPattern ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasAnacrusis ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:boolean
                             ] ,
